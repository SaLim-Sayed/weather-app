{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    
    
    
      "web": {
        "favicon": "./assets/favicon.png"
      },
      
        "experiments": {
          "tsconfigPaths": true
        },
      
      "plugins": [
        
        
        
      ],
    
    "orientation": "portrait",
    "icon": "./assets/ic_launcher.png",
    
     "userInterfaceStyle": "light",
    
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#132A52"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher.png",
        "backgroundColor": "#132A52"
      }
    }
  }
}
