{
  "expo": {
    "name": "weather app",
    "slug": "weather-app",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "backgroundColor": "#132A52",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "stretch",
      "backgroundColor": "#132A52",
      "imageWidth": 1024,
      "imageHeight": 1024
    },
    "plugins": [
     ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.salimsayed.weatherapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher.png",
        "backgroundColor": "#132A52"
      },
      "package": "com.salimsayed.weatherapp"
    },
    "extra": {
      "eas": {
        "projectId": "3e1b8c84-1561-49c1-9723-f4eb3a71b3ff"
      }
    }
  }
}
